{
  "compilerOptions": { 
    "target": "es6",                         
    "module": "es2020",                  
    "outDir": "./js",                 
    "rootDir": "./ts",                     
    "alwaysStrict": true,                  
    "moduleResolution": "node",            
    "esModuleInterop": true,                  
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true
  },
  "include": ["ts"],
}

